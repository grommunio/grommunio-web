<?xml version="1.0"?>
<!DOCTYPE plugin SYSTEM "manifest.dtd">
<plugin version="2">
	<info>
		<version>2.2</version>
		<name>filesbackendDefault</name>
		<title>Default backend for Files</title>
		<author>grommunio GmbH</author>
		<authorURL>https://www.grommunio.com</authorURL>
		<description>This backend provides the default WebDAV integration for Files</description>
	</info>
	<dependencies>
		<depends type="depends">
			<dependsname>files</dependsname>
		</depends>
	</dependencies>
	<components>
		<component>
			<files>
				<server>
				    <serverfile>php/class.plugindefault.php</serverfile>
				</server>
				<client>
					<clientfile load="release">js/filesbackendDefault.js</clientfile>
					<clientfile load="debug">js/filesbackendDefault-debug.js</clientfile>
					<clientfile load="source">js/DefaultBackend.js</clientfile>
					<clientfile load="source">js/ui/FilesShareDialogPanel.js</clientfile>
					<clientfile load="source">js/ui/FilesShareUserEditPanel.js</clientfile>
					<clientfile load="source">js/ui/FilesShareUserEditContentPanel.js</clientfile>
					<clientfile load="source">js/ui/FilesShareUserGrid.js</clientfile>
					<clientfile load="source">js/ui/FilesShareUserGridColumnModel.js</clientfile>
					<clientfile load="source">js/ui/UserGroupPredictorField.js</clientfile>
					<clientfile load="source">js/data/ShareGridStore.js</clientfile>
					<clientfile load="source">js/data/ResponseHandler.js</clientfile>
					<clientfile load="source">js/data/ShareGridRecord.js</clientfile>
					<clientfile load="source">js/data/singleton/ShareStore.js</clientfile>
					<clientfile load="source">js/data/RecipientTypes.js</clientfile>
				</client>
				<resources>
					<resourcefile load="release">resources/css/filesbackendDefault.css</resourcefile>
					<resourcefile load="debug">resources/css/filesbackendDefault.css</resourcefile>
					<resourcefile load="source">resources/css/filesbackendDefault.css</resourcefile>
				</resources>
			</files>
		</component>
	</components>
</plugin>
